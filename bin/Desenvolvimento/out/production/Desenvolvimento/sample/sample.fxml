<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<VBox spacing="10" fx:controller="sample.SampleController" xmlns:fx="http://javafx.com/fxml/1">

    <padding>
        <Insets top="5" right="5" bottom="5" left="5" />
    </padding>

    <HBox alignment="CENTER" spacing="10">
        <TitledPane collapsible="false" text="Markup - Grupo">
            <GridPane fx:id="grid" vgap="10" hgap="10">
                <Label text="Markup" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <Label text="Margem" GridPane.columnIndex="2" GridPane.rowIndex="0" />
                <Label text="Faturamento" GridPane.columnIndex="3" GridPane.rowIndex="0" />
            </GridPane>
        </TitledPane>
    </HBox>
</VBox>