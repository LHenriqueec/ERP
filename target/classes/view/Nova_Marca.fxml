<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane id="rootPane" fx:controller="controller.MarcaController"
		 stylesheets="/style/Grupo_Marca.css" xmlns:fx="http://javafx.com/fxml/1">
	
	<VBox id="vbox">
		<Label id="lblTitle" text="Cadastro Marca" />
		
		<HBox id="hbox_field" VBox.vgrow="ALWAYS">
			<Label text="Nome" />
			<TextField fx:id="txtNome" HBox.hgrow="ALWAYS" />
		</HBox>
		
		<HBox id="hbox_button">
			<Button id="btnSalvar" text="Salvar" onAction="#onSalvar" />
			<Button id="btnCancelar" text="Cancelar" onAction="#onCancelar" />
		</HBox>
	</VBox>
	
</StackPane>

