<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane stylesheets="/style/sample.css" fx:controller="controller.MainController"
	xmlns:fx="http://javafx.com/fxml/1">

	<center>
		<StackPane id="center_pane"></StackPane>
	</center>

	<left>
		<VBox id="lf_vbox">
			<Button id="btnHome" onAction="#onDashboard" />
			<Button id="btnProduto" onAction="#onProduto" />
			<Button id="btnFaturamento" onAction="#ontNovo_Produto" />
			<Button id="btnExit" onAction="#onExit" />
		</VBox>
	</left>

</BorderPane>

